import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n, petya, vasya, tonya, counter = 0;
        n=sc.nextInt();
    for (int i = 0; i < n; i++)
    {
        petya=sc.nextInt();
        vasya=sc.nextInt();
        tonya=sc.nextInt();
        if (petya + vasya + tonya >= 2)
        {
            counter++;
        }
    }
   System.out.println(counter);
    }
}
